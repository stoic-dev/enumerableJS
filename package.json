{
    "name": "enumerablejs",
    "version": "0.1.0",
    "description": "Enumerable data structures inspired by .NET Core",
    "main": "main.js",
    "scripts": {
        "build:dev": "webpack --config config/webpack/webpack.dev.js --progress --profile",
        "build:prod": "webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
        "build": "npm run build:dev",
        "clean:coverage": "npm run rimraf -- coverage",
        "clean:dist": "npm run rimraf -- dist",
        "clean:docs": "npm run rimraf -- docs",
        "clean:install": "npm run rimraf -- node_modules",
        "clean": "npm run clean:coverage && npm run clean:dist && npm run clean:docs && npm run clean:install",
        "coverage": "npm test && nyc report --reporter=lcov",
        "docs": "npm run docs:html && opn docs/index.html",
        "docs:html": "npm run typedoc -- --options ./typedoc.json --exclude \"**/*+(spec|main).ts\" --excludeExternals --out ./docs ./src",
        "eslint": "eslint",
        "jest": "jest --verbose --config=jest.config.json",
        "lint": "npm run tslint -- \"src/**/*.ts\" && npm run eslint -- \"config/**\"",
        "postinstall": "npm audit && npm run jest",
        "postversion": "git push && git push --tags && npm run clean:dist",
        "preversion": "npm test",
        "rimraf": "rimraf",
        "test": "npm run lint && npm audit && npm run jest && opn coverage/lcov-report/index.html",
        "tslint": "tslint",
        "typedoc": "typedoc",
        "version": "npm run build:prod"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stoic-dev/enumerableJS.git"
    },
    "author": "John R Harris II",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/stoic-dev/enumerableJS/issues"
    },
    "homepage": "https://github.com/stoic-dev/enumerableJS#readme",
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/register": "^7.0.0",
        "@types/jest": "^23.3.13",
        "@types/node": "^10.12.18",
        "@types/webpack-env": "^1.13.6",
        "babel-loader": "^8.0.5",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "eslint": "^5.12.1",
        "jest": "^23.6.0",
        "opn": "^5.4.0",
        "opn-cli": "^4.0.0",
        "optimize-js-plugin": "0.0.4",
        "prettier": "^1.15.3",
        "rimraf": "^2.6.3",
        "source-map-loader": "^0.2.4",
        "ts-jest": "^23.10.5",
        "ts-mockito": "^2.3.1",
        "tslib": "^1.9.3",
        "tslint": "^5.12.1",
        "typedoc": "^0.14.2",
        "typedoc-plugin-external-module-name": "^2.0.0",
        "typescript": "^3.2.4",
        "uglifyjs-webpack-plugin": "^2.1.1",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.2.1",
        "webpack-merge": "^4.2.1"
    }
}
